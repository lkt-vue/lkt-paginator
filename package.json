{
  "name": "lkt-paginator",
  "version": "1.1.3",
  "description": "A library for automatically load result pages from an HTTP API",
  "keywords": [
    "lkt",
    "lkt-web-tech",
    "ts",
    "typescript",
    "vue",
    "vue3",
    "js",
    "javascript",
    "button",
    "ui"
  ],
  "type": "module",
  "module": "./dist/build.js",
  "exports": {
    ".": {
      "import": "./dist/build.js"
    },
    "./styles": "./dist/style.css"
  },
  "types": "./dist/index.d.ts",
  "files": ["dist/*", "src/**/*.vue"],
  "license": "MIT",
  "sideEffects": false,
  "scripts": {
    "build": "rm -rf dist/*; vue-tsc --declaration --emitDeclarationOnly; vite build; tsc --project tsconfig.build.json; cp build/* dist/"
  },
  "author": "Antonio Ibáñez",
  "devDependencies": {
    "@babel/types": "^7.23.6",
    "@types/node": "^20.11.19",
    "@types/rollup": "^0.54.0",
    "@vitejs/plugin-vue": "^5.0.4",
    "rollup": "^4.9.6",
    "typescript": "^5.3.3",
    "vite": "^5.1.3",
    "vue": "^3.3.0",
    "vue-tsc": "^1.8.27"
  },
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "lkt-button": "^1.0.10",
    "lkt-control-tools": "^1.0.1",
    "lkt-data-state": "^1.0.8",
    "lkt-events": "^1.0.2",
    "lkt-http-client": "^1.0.11",
    "lkt-string-tools": "^1.0.1",
    "lkt-theme": "^1.0.4",
    "lkt-ts-interfaces": "^1.0.2",
    "lkt-vue-tools": "^1.0.1",
    "process": "^0.11.10",
    "sass": "^1.57.1",
    "vue": "^3.2.37"
  }
}
