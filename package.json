{
  "name": "lkt-paginator",
  "version": "1.0.2",
  "description": "A library for automatically load result pages from an HTTP API",
  "main": "src/index.ts",
  "module": "src/index.ts",
  "private": true,
  "files": [
    "lkt-paginator.css",
    "dist/*",
    "src/**/*.vue"
  ],
  "keywords": [
    "lkt",
    "lkt-web-tech",
    "ts",
    "typescript",
    "vue",
    "vue3",
    "js",
    "javascript",
    "button",
    "ui"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/lkt-paginator.es.js",
      "require": "./dist/lkt-paginator.umd.js"
    },
    "./dist/style.css": "./dist/style.css"
  },
  "types": "./dist/types/index.d.ts",
  "scripts": {
    "serve": "vite",
    "build": "vite build && vue-tsc --emitDeclarationOnly --project tsconfig.json && mv dist/src dist/types && rm dist/README.md.d.ts && rm dist/vite.config.d.ts",
    "build2": "vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test-coverage": "vitest run --coverage"
  },
  "author": "Antonio Ibáñez",
  "devDependencies": {
    "@babel/core": "^7.13.13",
    "@babel/preset-env": "^7.13.12",
    "@babel/types": "^7.18.9",
    "@testing-library/cypress": "^7.0.4",
    "@testing-library/dom": "^7.30.1",
    "@testing-library/jest-dom": "^5.11.10",
    "@types/node": "^18.6.2",
    "@typescript-eslint/eslint-plugin": "^5.36.2",
    "@typescript-eslint/parser": "^5.36.2",
    "@vitejs/plugin-vue": "^3.0.1",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "cypress": "^6.8.0",
    "eslint": "8.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-cypress": "^2.11.2",
    "eslint-plugin-html": "^7.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.3.2",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "prettier": "^2.7.1",
    "typescript": "^4.8.2",
    "vue-tsc": "^0.38.9"
  },
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "lkt-button": "^1.0.10",
    "lkt-control-tools": "^1.0.1",
    "lkt-data-state": "^1.0.8",
    "lkt-events": "^1.0.2",
    "lkt-http-client": "^1.0.11",
    "lkt-string-tools": "^1.0.1",
    "lkt-theme": "^1.0.4",
    "lkt-ts-interfaces": "^1.0.2",
    "lkt-vue-tools": "^1.0.1",
    "process": "^0.11.10",
    "sass": "^1.57.1",
    "vue": "^3.2.37"
  }
}
